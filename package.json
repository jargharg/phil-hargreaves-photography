    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/stylelint-module": "^5.1.0",
    "@prismicio/slice-simulator-vue": "^0.2.3",
    "@slicemachine/adapter-nuxt": "^0.3.15",
    "autoprefixer": "^10.4.15",
